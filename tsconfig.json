{
    "compilerOptions": {
      "target": "es5",
      "module": "esnext",
      "lib": ["es6", "dom"],
      "jsx": "react",
      "strict": true,
      "esModuleInterop": true,
      "types": ["jest", "@testing-library/react"],
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": false,
      "sourceMap": true
    },
    "include": ["src/**/*"]
  }
  